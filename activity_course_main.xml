<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/Course1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="@string/csc_415"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/Course2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center"
        android:text="@string/csc_201"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Course3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:text="@string/inf_286"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Course4"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:text="@string/bis_111"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Course5"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:text="@string/mat_385"
        android:textSize="18sp" />

</LinearLayout>